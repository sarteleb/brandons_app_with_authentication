<h1>Listing animal_licenses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number of pets</th>
      <th>All licensed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @animal_licenses.each do |animal_license| %>
      <tr>
        <td><%= animal_license.name %></td>
        <td><%= animal_license.number_of_pets %></td>
        <td><%= animal_license.all_licensed %></td>
        <td><%= link_to 'Show', animal_license %></td>
        <% if user_signed_in? %>
          <td><%= link_to 'Edit', edit_animal_license_path(animal_license) %></td>
          <td><%= link_to 'Destroy', animal_license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if user_signed_in? %>
  <%= link_to 'New Animal license', new_animal_license_path %>
<% end %>
